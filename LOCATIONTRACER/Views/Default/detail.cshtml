@using LOCATIONTRACER;
@{
    ViewBag.Title = "Image Location";

}
<head>
    <style>
        #map {
            height: 400px;
            width: 95%;
        }
    </style>
</head>
<div class="container mt-2">
    <h2 class="text-success"><b>Image Location</b></h2>
    @*<img src="@file">*@
    <h3 class="text-primary">@ViewBag.Out</h3>
    </div><br />


    <div id="map" style="margin-left:30px;"></div>
    <script>
      function initMap() {
        var uluru = {lat: @ViewBag.latitude, lng: @ViewBag.longitude};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 16,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA9VaQhna-JoLZtUvF74aPgcejxUXs74f0&callback=initMap">
    </script>